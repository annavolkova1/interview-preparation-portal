<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0">
  <servlet>
    <servlet-name>ScheduleController</servlet-name>
    <servlet-class>com.epam.firstProject.controller.ScheduleController</servlet-class>

  </servlet>
  <servlet-mapping>
    <servlet-name>ScheduleController</servlet-name>
    <url-pattern>/schedule</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>InterviewController</servlet-name>
    <servlet-class>com.epam.firstProject.controller.InterviewController</servlet-class>

  </servlet>
  <servlet-mapping>
    <servlet-name>InterviewController</servlet-name>
    <url-pattern>/interview</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RegistrationController</servlet-name>
    <servlet-class>com.epam.firstProject.controller.RegistrationController</servlet-class>

  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationController</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LogInController</servlet-name>
    <servlet-class>com.epam.firstProject.controller.LogInController</servlet-class>

  </servlet>
  <servlet-mapping>
    <servlet-name>LogInController</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>


</web-app>
